    "@types/node": "^18.15.5",
    "@types/pino": "^7.0.5",
    "@types/pino-http": "^5.8.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
